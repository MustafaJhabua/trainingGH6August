    </validationRules>
    <validationRules>
        <fullName>Trial_SIte_Zip_Code_must_be_5_or_9_digit</fullName>
        <active>true</active>
        <description>ENT-8543</description>
        <errorConditionFormula>AND( OR(Country__c = &quot;US&quot;), NOT(REGEX(Zip_Code__c , &quot;\\d{5}(-\\d{4})?&quot;)), 
$Profile.Id &lt;&gt; &quot;00e800000015KaN&quot; )</errorConditionFormula>
        <errorMessage>If country is US, ZIP must be in &quot;99999&quot; or &quot;99999-9999&quot; format.</errorMessage>
    </validationRules>
