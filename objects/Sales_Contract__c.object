        <errorMessage>Coverage Notes are required when choosing &quot;Other&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Contract_Field_Requirements</fullName>
        <active>true</active>
        <description>If Contract #, the remaining fields in the Sales Enterprise Contracts will be required.</description>
        <errorConditionFormula>AND
(NOT(ISBLANK(Contract__c)),
OR(
ISBLANK (Effective_Date__c),
OR(
ISBLANK (End_Date__c))))</errorConditionFormula>
        <errorDisplayField>Contract__c</errorDisplayField>
        <errorMessage>If Contract # is populated, Effective Date &amp; End Date are required.</errorMessage>
    </validationRules>
