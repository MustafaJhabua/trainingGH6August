    <validationRules>
        <fullName>lightsideapps__Minutes_Mutualls_exclusive</fullName>
        <active>true</active>
        <errorConditionFormula>AND 
( 
!ISBLANK(lightsideapps__BusinessMinutesBeforeAutoSign__c),
!ISBLANK(lightsideapps__Mins_Before_AutoSign__c)
)</errorConditionFormula>
        <errorMessage>Business Minutes Before Auto Sign and Mins Before Auto Sign are mutually exclusive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>lightsideapps__Minutes_Required</fullName>
        <active>true</active>
        <description>Ensures that either Minutes before sign or Business Minutes before sign are completed.</description>
        <errorConditionFormula>AND
(
ISBLANK(lightsideapps__BusinessMinutesBeforeAutoSign__c),
ISBLANK(lightsideapps__Mins_Before_AutoSign__c)
)</errorConditionFormula>
        <errorMessage>Business Minutes before Autosign or Minutes before Autosign must have a value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>lightsideapps__Sign_On_Behalf_of_User_Mutually_Exclusiv</fullName>
        <active>true</active>
        <errorConditionFormula>OR(

AND( 
  NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_Field__c)), 
  OR(
    NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_Queue__c)), 
    NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_User__c)) 
    )
  ),
AND( 
  NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_Queue__c)), 
  OR(
    NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_Field__c)), 
    NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_User__c))
  )
),
AND( 
  NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_User__c)), 
  OR(
    NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_Field__c)), 
    NOT(ISBLANK(lightsideapps__Sign_on_Behalf_of_Queue__c))
  )
)
)</errorConditionFormula>
        <errorMessage>Sign on Behalf of User, Sign on Behalf of Field and Sign on Behalf of Queue are mutually exclusive.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>lightsideapps__Sign_on_Behalf_of_required</fullName>
        <active>true</active>
        <errorConditionFormula>AND
(
 lightsideapps__Sign_on_Behalf_of_Field__c == null,
 lightsideapps__Sign_on_Behalf_of_Queue__c == null,
 lightsideapps__Sign_on_Behalf_of_User__c == null 
)</errorConditionFormula>
        <errorMessage>You must select a User, Field or Queue to sign on behalf of.</errorMessage>
    </validationRules>
