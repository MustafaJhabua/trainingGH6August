        <criteriaItems>
            <field>Guardant_360_Sample__c.Status__c</field>
            <operation>notEqual</operation>
            <value>CANCELLED,Amended,Addendum,Corrected</value>
        </criteriaItems>
        <criteriaItems>
            <field>Guardant_360_Sample__c.Final_Report_Date__c</field>
            <operation>greaterThan</operation>
            <value>2/1/2016</value>
        </criteriaItems>
        <criteriaItems>
            <field>Guardant_360_Sample__c.UE_Sample_Workflow__c</field>
            <operation>equals</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Guardant_360_Sample__c.Sample_Owner__c</field>
            <operation>notContain</operation>
            <value>FAUL,JURI</value>
        </criteriaItems>
        <description>This flow triggers an email, sent to the physician, upon reporting a No Alteration Found sample.</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
