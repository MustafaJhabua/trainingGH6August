    <rules>
        <fullName>Update SAC Trigger</fullName>
        <actions>
            <name>Update_SAC_Checkbox</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <formula>Date_Received__c  &gt;  Account__r.Agreement_Date__c</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Update SC Trigger</fullName>
        <actions>
            <name>Update_SAC_Checkbox</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <formula>Date_Received__c  &gt;= Account__r.Sales_Contract__r.Effective_Date__c
&amp;&amp;
NOT(ISPICKVAL(Account__r.Sales_Contract__r.Status__c,&quot;Terminated&quot;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
